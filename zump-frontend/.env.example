# Zump.fun Frontend Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# Starknet Configuration
# ===========================================

# Network to use: 'sepolia' (testnet) or 'mainnet'
REACT_APP_STARKNET_NETWORK=sepolia

# RPC URL for Starknet network
# Sepolia: https://starknet-sepolia.public.blastapi.io
# Mainnet: https://starknet-mainnet.public.blastapi.io
REACT_APP_STARKNET_RPC_URL=https://starknet-sepolia.public.blastapi.io

# ===========================================
# Contract Addresses (Sepolia Testnet)
# ===========================================
# Deploy contracts using: npm run deploy (from root directory)
# After deployment, copy the addresses here

# PumpFactory contract - manages token launches
REACT_APP_PUMP_FACTORY_ADDRESS=0x073399b589e513c4aad810f1b5fab0e8ee00d71e5c606730617197486c889f50

# ProtocolConfig contract - stores protocol parameters
REACT_APP_PROTOCOL_CONFIG_ADDRESS=0x0

# Quote token address (STRK on Sepolia)
REACT_APP_QUOTE_TOKEN_ADDRESS=0x04718f5a0fc34cc1af16a1cdee98ffb20c31f5cd61d6ab07201858f4287c938d

# Privacy contracts (optional - for stealth launches)
REACT_APP_STEALTH_GENERATOR_ADDRESS=0x0
REACT_APP_NULLIFIER_REGISTRY_ADDRESS=0x0
REACT_APP_ZK_VERIFIER_ADDRESS=0x0

# ===========================================
# Contract Addresses (Mainnet) - Optional
# ===========================================
# Only needed if deploying to mainnet

REACT_APP_STARKNET_RPC_URL_MAINNET=https://starknet-mainnet.public.blastapi.io
REACT_APP_PUMP_FACTORY_ADDRESS_MAINNET=0x0
REACT_APP_PROTOCOL_CONFIG_ADDRESS_MAINNET=0x0
REACT_APP_QUOTE_TOKEN_ADDRESS_MAINNET=0x04718f5a0fc34cc1af16a1cdee98ffb20c31f5cd61d6ab07201858f4287c938d
REACT_APP_STEALTH_GENERATOR_ADDRESS_MAINNET=0x0
REACT_APP_NULLIFIER_REGISTRY_ADDRESS_MAINNET=0x0
REACT_APP_ZK_VERIFIER_ADDRESS_MAINNET=0x0

# ===========================================
# Supabase Configuration
# ===========================================
# Required for storing token metadata and images

# 1. Create a Supabase project at https://supabase.com
# 2. Go to Project Settings > API
# 3. Copy the URL and anon key below

REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# ===========================================
# Optional Configuration
# ===========================================

# Enable debug logging (development only)
# REACT_APP_DEBUG=true

# Price polling interval in milliseconds (default: 10000)
# REACT_APP_PRICE_POLL_INTERVAL=10000
